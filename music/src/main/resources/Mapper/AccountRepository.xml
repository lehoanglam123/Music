<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.security.repository.AccountRepository">
    <select id="getAll">
        SELECT
            `account_id`,
            `username`,
            `password`,
            `gmail`,
            LEFT(`favorite`, 256),
            `role`
        FROM
            `music`.`account`
        WHERE
            1=1;
    </select>
</mapper>